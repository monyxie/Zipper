{
    "name": "monyxie/zipper",
    "type": "library",
    "description": "Drop-in replacement for Chumper/Zipper with Laravel 6.x support.",
    "keywords": ["laravel", "ZIP", "Archive"],
    "homepage": "http://github.com/monyxie/zipper",
    "license": "Apache2",
    "authors": [
        {
            "name": "Nils Plaschke",
            "email": "github@nilsplaschke.de",
            "homepage": "http://nilsplaschke.de",
            "role": "Original Developer"
        },
        {
            "name": "monyxie",
            "email": "monyxie@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "ext-zip": "*",
        "illuminate/support": "^6.0",
        "illuminate/filesystem": "^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "8.*"
    },
    "autoload": {
        "psr-4": {
            "Chumper\\Zipper\\": "src/Chumper/Zipper"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chumper\\Zipper\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chumper\\Zipper\\ZipperServiceProvider"
            ],
            "aliases": {
                "Zipper": "Chumper\\Zipper\\Zipper"
            }
        }
    },
    "replace": {
        "chumper/zipper": "1.0.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
